        priceLabel.innerHTML = "1-Year Plan <span class='line-through'>($99/year)</span> ($74/year) <b>with code 3FREE</b>";
        document.querySelector("button[name|='add']").dataset.promoOverride = "3free"
        document.querySelector(".form-radio label[for|='32194082472001'] input").click();
